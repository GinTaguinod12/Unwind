<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Outer dirt ring -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#5D4037" />
            <size android:width="100dp" android:height="100dp" />
        </shape>
    </item>
    <!-- Middle ring -->
    <item android:top="8dp" android:bottom="8dp" android:left="8dp" android:right="8dp">
        <shape android:shape="oval">
            <solid android:color="#4E342E" />
        </shape>
    </item>
    <!-- Inner dark hole -->
    <item android:top="16dp" android:bottom="16dp" android:left="16dp" android:right="16dp">
        <shape android:shape="oval">
            <solid android:color="#2E1B18" />
        </shape>
    </item>
    <item android:top="24dp" android:bottom="24dp" android:left="24dp" android:right="24dp">
        <shape android:shape="oval">
            <solid android:color="#1B0000" />
        </shape>
    </item>
</layer-list>